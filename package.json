{
  "name": "@formschema/native",
  "version": "2.0.0-alpha.1",
  "description": "Vue component form based on JSON Schema",
  "main": "dist/FormSchema.js",
  "scripts": {
    "demo": "cd demo && npm run serve",
    "pretest": "npm run lint",
    "test": "vue-cli-service test --colors --testMatch '<rootDir>/test/specs/*.spec.js'",
    "lint": "vue-cli-service lint",
    "prepublish": "npm test",
    "coveralls": "jest --coverage && cat ./coverage/lcov.info | coveralls",
    "readme": "vuedoc.md --join src/components/*.js --ignore-name --ignore-data --section 'FormSchema API' --output README.md",
    "build": "vue-cli-service build --target lib && npm run readme"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/formschema/native.git"
  },
  "keywords": [
    "vue",
    "form",
    "json",
    "schema"
  ],
  "author": "SÃ©bastien Demanou",
  "license": "MIT",
  "bugs": {
    "url": "https://gitlab.com/formschema/native/issues"
  },
  "homepage": "https://gitlab.com/formschema/native",
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.42",
    "@babel/plugin-transform-runtime": "^7.0.0-beta.42",
    "@babel/preset-es2015": "^7.0.0-beta.42",
    "@babel/preset-stage-2": "^7.0.0-beta.42",
    "@vue/cli-plugin-babel": "^3.0.0-beta.6",
    "@vue/cli-plugin-eslint": "^3.0.0-beta.6",
    "@vue/cli-plugin-unit-jest": "^3.0.0-beta.6",
    "@vue/cli-service": "^3.0.0-beta.6",
    "@vue/eslint-config-standard": "^3.0.0-beta.6",
    "@vue/test-utils": "^1.0.0-beta.12",
    "@vuedoc/md": "^1.1.1",
    "babel-core": "^6.26.0",
    "babel-jest": "^22.4.3",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "coveralls": "^3.0.0",
    "regenerator-runtime": "^0.11.1",
    "sinon": "^4.4.8",
    "vue": "^2.5.16"
  },
  "dependencies": {},
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
