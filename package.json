{
  "name": "vue-json-schema",
  "version": "2.0.0-alpha1",
  "description": "Vue component form based on JSON Schema",
  "main": "dist/vue-json-schema.js",
  "scripts": {
    "pretest": "eslint --ext .js src test/specs",
    "test": "jest --colors",
    "coveralls": "jest --coverage && cat ./coverage/lcov.info | coveralls",
    "posttest": "make readme",
    "prepublish": "npm test",
    "build": "npm test && node build",
    "tag": "git tag v`node -p 'require(\"./package\").version'`"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/demsking/vue-json-schema.git"
  },
  "keywords": [
    "vue",
    "form",
    "json",
    "schema"
  ],
  "author": "SÃ©bastien Demanou",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/demsking/vue-json-schema/issues"
  },
  "homepage": "https://github.com/demsking/vue-json-schema#readme",
  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.11",
    "@vuedoc/md": "^1.1.1",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^22.0.6",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "coveralls": "^3.0.0",
    "eslint": "^4.15.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^4.0.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "jasmine-core": "^2.8.0",
    "jest": "^22.0.6",
    "ora": "^1.3.0",
    "phantomjs-prebuilt": "^2.1.16",
    "regenerator-runtime": "^0.11.1",
    "rimraf": "^2.6.2",
    "vue": "^2.5.13",
    "vue-template-compiler": "^2.5.13",
    "webpack": "^3.10.0"
  },
  "dependencies": {
    "lodash": "^4.17.10"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,vue}",
      "!.eslintrc.js",
      "!build.js",
      "!webpack.config.js",
      "!dist/**",
      "!coverage/**",
      "!**/node_modules/**"
    ],
    "mapCoverage": true
  }
}
